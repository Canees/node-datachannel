{
  "name": "node-datachannel",
  "version": "0.0.4",
  "description": "libdatachannel node bindings",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "install": "prebuild-install || ncmake rebuild -- -DNO_EXAMPLES=1",
