  },
  "homepage": "https://github.com/murat-dogan/node-datachannel#readme",
  "dependencies": {
    "cmake-js": "^6.1.0",
    "nan": "^2.14.1",
    "napi-thread-safe-callback": "0.0.6",
    "node-addon-api": "^3.0.1",
    "prebuild-install": "^5.3.5"
  },
  "devDependencies": {},
